{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "version": 2,
    "blocks": [
      {
        "alignment": "left",
        "newline": true,
        "type": "prompt",
        "segments": [
          {
            "foreground": "#b1ff4f",
            "style": "powerline",
            "template": "[üêç {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}] ",
            "type": "python"
          },
          {
            "foreground": "#ffaed8",
            "style": "plain",
            "template": "<#eaeaea>‚éß</> ‚å©{{.Path}} ‚å™",
            "type": "path",
            "properties": {
              "folder_separator_icon": " | ",
              "home_icon": "Ôë≠",
              "style": "letter"
            }
          },
          {
            "foreground": "#62beff",
            "style": "plain",
            "template": "‚ü®{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} ‚àÜ{{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}‚ü©",
            "type": "git",
            "properties": {
              "branch_icon": "Ôêò",
              "branch_identical_icon": "‚â°",
              "fetch_status": true
            }
          },
          {
            "foreground": "#98C379",
            "style": "plain",
            "template": "<#ffffff>‚ó¶</> ‚ò¢{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
            "type": "node",
            "properties": {
              "fetch_version": true
            }
          }
        ]
      },
      {
        "alignment": "right",
        "type": "prompt",
        "segments": [
          {
            "foreground": "#fb0207",
            "style": "plain",
            "template": " √ó√ó√ó ",
            "type": "status"
          },
          {
            "foreground": "#9966ff",
            "style": "plain",
            "template": "‚ü®{{ .FormattedMs }}‚ü© <#eaeaea>‚é´</>",
            "type": "executiontime",
            "properties": {
              "style": "austin",
              "threshold": 0
            }
          }
        ]
      },
      {
        "alignment": "left",
        "newline": true,
        "type": "prompt",
        "segments": [
          {
            "foreground": "#3ce6bf",
            "style": "plain",
            "template": "<#eaeaea>‚é©</> ‚ü© ",
            "type": "text"
          }
        ]
      },
      {
        "alignment": "right",
        "type": "rprompt",
        "segments": [
          {
            "foreground": "#f5f5a6",
            "style": "plain",
            "template": "‚ü®{{ .CurrentDate | date .Format }}‚ü© <#eaeaea>‚é≠</>",
            "type": "time",
            "properties": {
              "time_format": "15:04:05"
            }
          }
        ]
      }
    ],
    "transient_prompt": {
      "foreground": "#3ce6bf",
      "template": "  ‚ü© "
    }
  }